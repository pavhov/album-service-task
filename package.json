{
  "name": "album-service",
  "version": "1.0.0",
  "description": "album-service",
  "main": "src/Main.ts",
  "bin": "src/Main.ts",
  "scripts": {
    "start": "ts-node --project tsconfig.json src/Main.ts",
    "dev": "ts-node-dev --project tsconfig.json --watch --respawn src/Main.ts",
    "lint": "eslint src/**/*.ts --fix",
    "upgrade": "ncu -u && npm i"
  },
  "nodemonConfig": {
    "delay": 700,
    "cwd": "./",
    "watch": [
      "resources",
      "src",
      "out"
    ],
    "ext": "ts,json,json5",
    "ignore": [
      "src/**/*.spec.ts"
    ],
    "execMap": {
      "ts": "./node_modules/.bin/ts-node"
    }
  },
  "keywords": [],
  "author": "pavhov93@gmail.com",
  "license": "ISC",
  "dependencies": {
    "ajv": "^8.8.2",
    "ajv-formats": "^2.1.1",
    "bluebird": "^3.7.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-list-endpoints": "^6.0.0",
    "json5": "^2.2.2",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-basic-auth": "^4.0.0",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.1.1",
    "koa-send": "^5.0.1",
    "kraken-api": "^1.0.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "mongodb": "^4.2.0",
    "node-fetch": "^2.6.7",
    "objectid": "^3.2.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.2.0",
    "nodemon": "^2.0.15",
    "npm-check-updates": "^12.0.2",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2"
  },
  "optionalDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/express-list-endpoints": "^6.0.0",
    "@types/json5": "0.0.30",
    "@types/koa": "^2.13.4",
    "@types/koa-basic-auth": "^2.0.4",
    "@types/koa-bodyparser": "^4.3.4",
    "@types/koa-router": "^7.4.4",
    "@types/koa-send": "^4.1.3",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.10",
    "@types/node-fetch": "^2.5.12"
  }
}
